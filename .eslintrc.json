{
  
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "rules": {
    "import/order": [
      "warn",
      {
        "groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object"],
        "newlines-between": "always"
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "app/**/page.tsx",
        "app/**/layout.tsx",
        "app/**/loading.tsx",
        "app/**/error.tsx",
        "app/**/not-found.tsx"
      ],
      "rules": {
        "import/no-unused-modules": "off"
      }
    }
  ] 
  
  
