{

  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "rules": {
    // ✅ Supprimer le faux positif "React is not defined"
    "no-undef": "off",

    // ✅ Corriger les faux positifs sur page.tsx, layout.tsx, etc.
    "import/no-unused-modules": "off",

    // ✅ Optionnel : si trop strict
    "no-unused-vars": "warn",
    "import/order": "warn",
    "import/no-duplicates": "warn",
    "import/no-named-as-default": "warn"
  }

